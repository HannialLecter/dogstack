{
  "name": "dogstack",
  "version": "0.0.5",
  "description": "a popular-choice grab-bag framework for teams working on production web apps",
  "main": "index.js",
  "scripts": {
    "start": "node examples/todos/",
    "build-example-todos": "browserify examples/todos/ -o examples/todos/public/bundle.js",
    "dev": "npm run build-example-todos && npm run start"
  },
